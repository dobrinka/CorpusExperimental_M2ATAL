document.getElementById("searchtype").value; switch (type) { case "newspaper":
document.location.href="http://www.canada.com/nanaimodailynews/search_results.
html?searchtype=0&searchfor=" + escape(q); break; case "marketplace": document
.location.href="http://shopping.nanaimodailynews.canada.com/shared/search.aspx
?s=" + escape(q); break; case "canada": document.location.href="http://www.can
ada.com/findit/search/site/index.html?q=" + escape(q); break; case "fp":
